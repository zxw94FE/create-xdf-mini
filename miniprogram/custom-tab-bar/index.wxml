<view class="wrapper">
  <t-tab-bar value="{{value}}" bindchange="onChange" class="custom-tabbar">
    <t-tab-bar-item
      class="custom-tabbar-item"
      wx:for="{{list}}"
      wx:key="index"
      value="{{item.value}}"
      icon="{{item.icon}}"
      ariaLabel="{{item.ariaLabel}}"
    >{{ item.label }}</t-tab-bar-item>
  </t-tab-bar>
</view>
